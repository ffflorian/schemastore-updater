{
  "author": "Florian Imdahl <git@ffflorian.de>",
  "bin": {
    "schemastore-updater": "dist/cli.js"
  },
  "dependencies": {
    "commander": "10.0.0",
    "fs-extra": "11.1.0",
    "json-schema-to-typescript": "11.0.3",
    "jsonabc": "2.3.1",
    "semver": "7.3.8"
  },
  "devDependencies": {
    "@evilmartians/lefthook": "1.2.9",
    "@ffflorian/eslint-config": "0.8.2",
    "@ffflorian/prettier-config": "0.1.4",
    "@types/fs-extra": "11.0.1",
    "@types/jsonabc": "2.3.1",
    "@types/lodash": "4.14.191",
    "@types/node": "~18",
    "@types/prettier": "2.7.2",
    "@types/semver": "7.3.13",
    "@typescript-eslint/eslint-plugin": "4.0.0",
    "@typescript-eslint/parser": "3.10.1",
    "babel-eslint": "10.1.0",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jasmine": "4.1.3",
    "eslint-plugin-jsdoc": "40.0.0",
    "eslint-plugin-no-unsanitized": "4.0.2",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "logdown": "3.3.1",
    "prettier": "2.8.4",
    "rimraf": "4.1.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">= 10.9"
  },
  "files": [
    "dist",
    "settings.json"
  ],
  "license": "GPL-3.0",
  "main": "dist/index.js",
  "name": "schemastore-updater",
  "prettier": "@ffflorian/prettier-config",
  "repository": "https://github.com/ffflorian/schemastore-updater.git",
  "scripts": {
    "build": "tsc",
    "clear": "rimraf dist",
    "check-disabled": "yarn start check-disabled",
    "check-versions": "yarn start check-versions",
    "dist": "yarn clear && yarn build",
    "fix": "yarn fix:other && yarn fix:ts",
    "fix:other": "yarn prettier --write",
    "fix:ts": "yarn lint:ts --fix",
    "lint": "yarn lint:ts && yarn lint:other",
    "lint:other": "yarn prettier --list-different",
    "lint:ts": "eslint --ignore-path .gitignore --ext .ts src/",
    "prettier": "prettier \"*.{json,md,yml}\"",
    "start": "cross-env NODE_DEBUG='schemastore-updater/*' ts-node src/cli.ts",
    "test": "exit 0",
    "update": "yarn start update"
  },
  "version": "2.0.0"
}
